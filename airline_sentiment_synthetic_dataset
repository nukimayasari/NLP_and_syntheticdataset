n            feature_names.extend(columns)\n        else:\n            # handle OneHotEncoder inside pipelines\n            try:\n                if hasattr(transformer, 'named_steps'):\n                    last_step = transformer.named_steps[list(transformer.named_steps)[-1]]\n                    if hasattr(last_step, 'get_feature_names_out'):\n                        names = last_step.get_feature_names_out(columns)\n                        feature_names.extend(names)\n                    else:\n                        feature_names.extend(columns)\n                else:\n                    feature_names.extend(columns)\n            except:\n                feature_names.extend(columns)\n    return feature_names\n\ntransformed_feature_names = get_feature_names(preprocessor)\n\n# Step 4: Use TreeExplainer on the actual CatBoost model\nimport shap\nexplainer = shap.TreeExplainer(model)\nshap_values = explainer.shap_values(X_train_transformed)\n\n# Step 5: Calculate mean SHAP values\nmean_shap_values = np.mean(np.abs(np.stack(shap_values)), axis=(0, 1))\n\n# Step 6: Set threshold and pick important features\nthreshold = 0.05\nimportant_features = [transformed_feature_names[i] for i in range(len(mean_shap_values)) if mean_shap_values[i] > threshold]\n\nprint(\"Important features based on SHAP:\", important_features)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-19T18:44:44.181523Z","iopub.execute_input":"2025-05-19T18:44:44.181946Z","iopub.status.idle":"2025-05-19T18:44:45.691666Z","shell.execute_reply.started":"2025-05-19T18:44:44.181923Z","shell.execute_reply":"2025-05-19T18:44:45.690111Z"}},"outputs":[{"name":"stdout","text":"Important features based on SHAP: ['tweet_id', 'airline_sentiment_confidence', 'negativereason_confidence', 'negativereason_Customer Service Issue', 'airline_Delta', 'airline_US Airways']\n","output_type":"stream"}],"execution_count":17},{"cell_type":"code","source":"# Step 1: Create column lists\nnumeric_features = ['tweet_id', 'airline_sentiment_confidence', 'negativereason_confidence']\ncategorical_features = ['negativereason', 'airline']  # keep whole cols, not OHE names\n\n# Step 2: Create new preprocessor\nfrom sklearn.compose import ColumnTransformer\nfrom sklearn.preprocessing import OneHotEncoder, StandardScaler\nfrom sklearn.pipeline import Pipeline as SkPipeline\n\npreprocessor_shap = ColumnTransformer(\n    transformers=[\n        ('num', StandardScaler(), numeric_features),\n        ('cat', OneHotEncoder(handle_unknown='ignore'), categorical_features)\n    ]\n)\n\n# Step 3: Rebuild the pipeline with SHAP-selected features\npipeline_shap = SkPipeline(steps=[\n    ('preprocessor', preprocessor_shap),\n    ('classifier', CatBoostClassifier(verbose=0, auto_class_weights='Balanced', random_state=42))\n])\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-19T18:44:45.692753Z","iopub.execute_input":"2025-05-19T18:44:45.693165Z","iopub.status.idle":"2025-05-19T18:44:45.701221Z","shell.execute_reply.started":"2025-05-19T18:44:45.693134Z","shell.execute_reply":"2025-05-19T18:44:45.700066Z"}},"outputs":[],"execution_count":18},{"cell_type":"code","source":"param_grid_shap = {\n    'classifier__depth': [4, 6],\n    'classifier__learning_rate': [0.05, 0.1],\n    'classifier__iterations': [300, 500],\n    'classifier__l2_leaf_reg': [3, 5]\n}\n\ngrid_search_shap = GridSearchCV(\n    estimator=pipeline_shap,\n    param_grid=param_grid_shap,\n    scoring='accuracy',\n    cv=3,\n    n_jobs=-1\n)\n\ngrid_search_shap.fit(X_train, y_train)\nbest_model_shap = grid_search_shap.best_estimator_\n\nprint(\"Best SHAP-Selected Parameters:\", grid_search_shap.best_params_)\nprint(\"Best SHAP CV Accuracy:\", grid_search_shap.best_score_)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-19T18:44:45.702648Z","iopub.execute_input":"2025-05-19T18:44:45.70369Z","iopub.status.idle":"2025-05-19T18:46:05.470113Z","shell.execute_reply.started":"2025-05-19T18:44:45.703608Z","shell.execute_reply":"2025-05-19T18:46:05.468978Z"}},"outputs":[{"name":"stdout","text":"Best SHAP-Selected Parameters: {'classifier__depth': 4, 'classifier__iterations': 300, 'classifier__l2_leaf_reg': 5, 'classifier__learning_rate': 0.1}\nBest SHAP CV Accuracy: 0.8428087226125426\n","output_type":"stream"}],"execution_count":19},{"cell_type":"code","source":"from sklearn.metrics import confusion_matrix, ConfusionMatrixDisplay\n\n# Predict\ny_pred_shap = best_model_shap.predict(X_test)\n\n# Confusion Matrix\ncm = confusion_matrix(y_test, y_pred_shap, labels=best_model_shap.classes_)\ndisp = ConfusionMatrixDisplay(confusion_matrix=cm, display_labels=best_model_shap.classes_)\ndisp.plot(cmap='Blues')\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-19T18:46:05.471151Z","iopub.execute_input":"2025-05-19T18:46:05.471586Z","iopub.status.idle":"2025-05-19T18:46:05.752704Z","shell.execute_reply.started":"2025-05-19T18:46:05.471558Z","shell.execute_reply":"2025-05-19T18:46:05.751663Z"}},"outputs":[{"execution_count":20,"output_type":"execute_result","data":{"text/plain":"<sklearn.metrics._plot.confusion_matrix.ConfusionMatrixDisplay at 0x7b78904f8290>"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}}],"execution_count":20},{"cell_type":"markdown","source":"### 🧩 Confusion Matrix Breakdown\n\nBased on this:\n\n```\n           Predicted\n          | neg | neu | pos\n    -------------------------\ntrue neg | 1829 |  0  |  0  \ntrue neu |   0  | 264 | 355\ntrue pos |   0  | 104 | 368\n```\n\nWe can calculate:\n\n| Sentiment | Precision | Recall | F1-score | Notes |\n|-----------|-----------|--------|----------|-------|\n| **Negative** | 1.00 | 1.00 | 1.00 | Perfect classification 🙌 |\n| **Neutral** | 0.717 | 0.424 | ~0.53 | Many neutrals misclassified as positive |\n| **Positive** | 0.509 | 0.779 | ~0.616 | Slightly better than neutral, decent recall |\n\